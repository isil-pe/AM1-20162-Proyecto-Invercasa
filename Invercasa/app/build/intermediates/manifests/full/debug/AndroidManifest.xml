<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vrm.invercasa"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <application
        android:name="com.vrm.invercasa.InvercasaApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.vrm.invercasa.WelcomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.vrm.invercasa.LoginActivity"
            android:label="@string/app_name" />
        <activity android:name="com.vrm.invercasa.MainActivity" />
        <activity
            android:name="com.vrm.invercasa.SignUpActivity"
            android:label="@string/title_activity_sign_up" />
        <activity
            android:name="com.vrm.invercasa.TermsActivity"
            android:label="@string/title_activity_terms" />
    </application>

</manifest>